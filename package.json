  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "browserify": "^14.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0",
    "reactify": "^1.1.1",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "babelify": "^7.3.0",
